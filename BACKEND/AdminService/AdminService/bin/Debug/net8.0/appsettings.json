{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=p04_rent_it;User=root;Password=root;"
  },
  "Jwt": {
    "SecretKey": "rentit_super_secret_key_rentit_super_secret_key",
    "Issuer": "AdminService",
    "Audience": "AdminServiceClient",
    "ExpirationHours": "24"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
  {
    "spring": {
      "application": {
        "name": "AdminService"
      }
    },
    "eureka": {
      "client": {
        "serviceUrl": "http://localhost:8761/eureka/", //Eureka service
        "shouldRegisterWithEureka": true,
        "shouldFetchRegistry": false
      },
      "instance": {
        "port": 5046 // mention your port no of http url
      }
    }
  }
}

