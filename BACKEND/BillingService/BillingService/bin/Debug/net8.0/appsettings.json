{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=p04_rent_it;user=root;password=root"
  },
  "JwtSettings": {
    "Secret": "rentit_super_secret_key_rentit_super_secret_key",
    "Issuer": "RentIt",
    "Audience": "RentIt",
    "ExpirationHours": 24
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000"
    ]
  },

  "spring": {
    "application": {
      "name": "BillingService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true
    },
    "instance": {
      "port": 7006,
      "hostname": "localhost",
      "preferIpAddress": true,
      "instanceId": "BillingService:7006",
      "healthCheckUrlPath": "/health",
      "statusPageUrlPath": "/info"
    }
  }
}