{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "spring": {
    "application": {
      "name": "SearchService"
    }
  },

  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true
    },
    "instance": {
      "hostname": "localhost",
      "nonSecurePort": 7260,
      "isSecurePortEnabled": false,
      "preferIpAddress": true
    }
  }
}

